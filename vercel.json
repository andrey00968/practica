{
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" },
      { "src": "templates/**/*", "use": "@vercel/static" },
      { "src": "static/css/**/*", "use": "@vercel/static" },
      { "src": "static/js/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/css/(.*)", "dest": "static/css" },
      { "src": "/static/js/(.*)", "dest": "static/js" },
      { "src": "/(.*)", "dest": "app.py" }
    ]
  }